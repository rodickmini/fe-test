'use strict'

//数组去重

let arr = [1, 2, 22, 2, 6, 66, 6, 99]

let map = {}, dest = []
arr.forEach(function(el) {
  if(map[el] === undefined) {
    dest.push(el)
    map[el] = 1
  }
})

console.log(dest)